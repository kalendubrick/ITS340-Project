import javax.swing.JFrame;


public class GameTable extends JFrame{

	public static void main(String[] args){
		
		JFrame gameTable = new GameTable();
		gameTable.setTitle("Texas Hold'em");
		gameTable.setSize(1024, 768);
		gameTable.setLocation(500, 500);
		gameTable.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		gameTable.setVisible(true);
		
	}
}
